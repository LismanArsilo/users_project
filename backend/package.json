{
  "name": "backend",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "development": "nodemon --exec babel-node ./server/index.js",
    "clean": "rm -rf build && mkdir build",
    "cleanw": "rmdir /S build && mkdir build",
    "start": "node ./build/index.js",
    "build-server": "babel -d ./build ./server -s",
    "build": "npm run clean && npm run build-server",
    "buildw": "npm run cleanw && npm run build-server",
    "test": "jest",
    "sql": "npx sequelize-cli db:seed:all",
    "reverse:db": "sequelize-auto -o ./schemas -d codeiddb -h localhost -u postgres -p 5432 -x admin -e postgres -l esm"
  },
  "dependencies": {
    "bcrypt": "^5.0.1",
    "body-parser": "^1.20.0",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "express-pino-logger": "^7.0.0",
    "express-session": "^1.17.3",
    "formidable": "^2.0.1",
    "graphql": "^16.5.0",
    "graphql-tools": "^8.3.2",
    "helmet": "^5.1.1",
    "pg": "^8.7.3",
    "sequelize": "^6.21.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.18.10",
    "@babel/core": "^7.18.9",
    "@babel/node": "^7.18.9",
    "@babel/plugin-transform-runtime": "^7.18.5",
    "@babel/preset-env": "^7.18.9",
    "@babel/runtime": "^7.18.9",
    "nodemon": "^2.0.19",
    "sequelize-auto": "^0.8.8"
  }
}
